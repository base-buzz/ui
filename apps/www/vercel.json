{
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=8192",
      "TURBO_REMOTE_ONLY": "true",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "git": {
    "submodules": true,
    "deploymentEnabled": {
      "main": true
    }
  },
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "outputDirectory": ".next"
}
